{
  "name": "sicte-digital-turn",
  "version": "0.0.1",
  "description": "Digital turn system to use at the entrance of SICTE's main offices",
  "productName": "Digital Turn App",
  "author": "departamento.tecnologia@sicte.com",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./ --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,css,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "cross-env ENV=dev quasar dev -m electron",
    "build:co": "cross-env ENV=prod FOLDER=co quasar build -m electron",
    "build:pl": "cross-env ENV=prod FOLDER=pl quasar build -m electron",
    "build:pt": "cross-env ENV=prod FOLDER=pt quasar build -m electron",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.3.0",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "electron": "^26.0.0",
    "electron-packager": "^17.1.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}